ranksum percexits5y if CVCtype==3 | CVCtype==5, by(CVCtype)
ranksum percexits5y if CVCtype==3 | CVCtype==6, by(CVCtype)
ranksum percexits5y if CVCtype==4 | CVCtype==5, by(CVCtype)
ranksum percexits5y if CVCtype==4 | CVCtype==6, by(CVCtype)
ranksum percexits5y if CVCtype==5 | CVCtype==6, by(CVCtype)
ranksum percexits5y if CVCtype==1 | CVCtype==7, by(CVCtype)
ranksum percexits5y if CVCtype==2 | CVCtype==7, by(CVCtype)
ranksum percexits5y if CVCtype==3 | CVCtype==7, by(CVCtype)
ranksum percexits5y if CVCtype==4 | CVCtype==7, by(CVCtype)
ranksum percexits5y if CVCtype==5 | CVCtype==7, by(CVCtype)
ranksum percexits5y if CVCtype==6 | CVCtype==7, by(CVCtype)

ranksum percstrategic5y if CVCtype==1 | CVCtype==2, by(CVCtype)
